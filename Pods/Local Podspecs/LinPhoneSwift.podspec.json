{
  "name": "LinPhoneSwift",
  "version": "1.0.0",
  "summary": "Linphone for Swift",
  "license": "MIT",
  "authors": {
    "Alsey Coleman Miller": "colemancda.github.io"
  },
  "homepage": "http://github.com/coleman/LinPhoneSwift",
  "description": "Swift library for Linphone",
  "requires_arc": true,
  "platforms": {
    "ios": "8.0"
  },
  "source": {
    "path": "*"
  },
  "vendored_frameworks": "ios/*.framework",
  "preserve_paths": [
    "include/*.h",
    "include/*/*.h"
  ],
  "xcconfig": {
    "ENABLE_BITCODE": "NO",
    "SWIFT_INCLUDE_PATHS": "${PODS_ROOT}/LinPhoneSwift/include/**"
  }
}
